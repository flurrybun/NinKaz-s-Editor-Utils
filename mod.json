{
    "geode": "4.0.1",
    "gd": {
        "win": "2.2074",
        "android": "2.2074",
        "mac": "2.2074",
        "ios": "2.2074"
    },
    "version": "v1.1.0",
    "id": "ninkaz.editor_utils",
    "name": "NinKaz's Editor Utils",
    "developer": "NinKaz",
    "description": "Editor utilities to make my life easier",
    "links": {
        "source": "https://github.com/flurrybun/ninkaz-editor-utils"
    },
    "tags": ["editor", "interface", "utility"],
    "dependencies": [
        {
            "id": "geode.node-ids",
            "version": ">=v1.17.0",
            "importance": "required"
        }
    ],
    "resources": {
        "spritesheets": {
            "MultiEditSheet": ["resources/multi-edit/*.png"]
        }
    },
    "settings": {
        "select-mixed-input": {
            "name": "Enter Multi-Edit Mode",
            "description": "Change the method used to enter multi-edit mode.",
            "type": "string",
            "one-of": ["Right Click", "Button", "Right Click & Button"],
            "default": "Right Click",
            "platforms": ["desktop"]
        }
    }
}
